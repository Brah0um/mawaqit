{% trans_default_domain 'FOSUserBundle' %}

<div class="col-md-6">
{{ form_start(form, { 'action': path('fos_user_profile_edit'), 'attr': { 'class': 'fos_user_profile_edit' } }) }}
{{ form_widget(form) }}

<div>
    <button type="submit" class="btn btn-lg btn-primary">{{'profile.edit.submit'|trans}}</button>
    <a href="{{path("mosque_index")}}" class="btn btn-lg btn-danger">{{'cancel'|trans({}, 'messages')}}</a>
</div>  

{{ form_end(form) }}
</div>  

